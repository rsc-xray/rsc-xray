{
  "name": "@rsc-xray/analyzer",
  "version": "1.0.0",
  "description": "Static analyzer for React Server Components: detect server/client boundaries, bundle sizes, forbidden imports, and performance issues",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react-server-components",
    "rsc",
    "nextjs",
    "debug",
    "observability",
    "static-analysis",
    "suspense",
    "bundles",
    "bytes"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "test": "vitest --run"
  },
  "dependencies": {
    "@rsc-xray/schemas": "workspace:*",
    "typescript": "^5.6.0"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsc-xray/rsc-xray"
  },
  "license": "MIT"
}
