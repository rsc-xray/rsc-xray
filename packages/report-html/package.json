{
  "name": "@rsc-xray/report-html",
  "version": "0.2.3",
  "description": "Generate offline HTML reports for RSC X-Ray analysis: visualize boundaries, Suspense, bundle sizes, and optimization suggestions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react-server-components",
    "rsc",
    "nextjs",
    "debug",
    "observability",
    "report",
    "html",
    "visualization"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "test": "vitest --run --passWithNoTests"
  },
  "dependencies": {
    "@rsc-xray/schemas": "workspace:^"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsc-xray/rsc-xray"
  },
  "license": "MIT"
}
